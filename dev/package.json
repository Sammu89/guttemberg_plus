{
  "name": "guttemberg-plus",
  "version": "1.0.0",
  "description": "Advanced Gutenberg blocks: Accordion, Tabs, and Table of Contents with powerful theming system",
  "author": "Your Name",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "wordpress",
    "gutenberg",
    "blocks",
    "accordion",
    "tabs",
    "table-of-contents"
  ],
  "homepage": "https://github.com/yourusername/guttemberg-plus",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/guttemberg-plus.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/guttemberg-plus/issues"
  },
  "scripts": {
    "validate:sync": "node build-tools/validators/validate-schema-code-sync.js && node build-tools/validators/validate-generated-code-completeness.js",
    "validate:sync:warn": "node build-tools/validators/validate-schema-code-sync.js --warn-only && node build-tools/validators/validate-generated-code-completeness.js --warn-only",
    "schema:build": "node build-tools/schema-compiler.js",
    "schema:build:warn": "node build-tools/schema-compiler.js --warn-only",
    "prebuild": "npm run schema:build:warn && npm run validate:sync:warn",
    "build": "wp-scripts build",
    "postbuild": "node build-tools/rename-css-files.js && node build-tools/prompt-dist.js",
    "dist": "DIST_YES=1 npm run build",
    "dist:win": "set DIST_YES=1&& npm run build",
    "prestart": "npm run schema:build",
    "start": "wp-scripts start",
    "lint:js": "wp-scripts lint-js",
    "lint:css": "wp-scripts lint-style",
    "format": "wp-scripts format",
    "packages-update": "wp-scripts packages-update",
    "plugin-zip": "wp-scripts plugin-zip"
  },
  "devDependencies": {
    "@wordpress/icons": "^11.4.0",
    "@wordpress/scripts": "^27.0.0",
    "jsdom": "^27.4.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "lucide-react": "^0.562.0"
  }
}
